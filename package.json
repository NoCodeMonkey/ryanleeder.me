{
  "name": "ryanleeder.me",
  "version": "1.0.0",
  "description": "My personal portfolio website powered by node.js",
  "author": "Ryan Leeder",
  "license": "Apache",
  "keywords": [
    "css",
    "sass",
    "bootstrap",
    "responsive",
    "node.js"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nocodemonkey/ryanleeder.me.git"
  },
  "scripts": {
    "start": "node server",
    "css:lint": "stylelint --config build/.stylelintrc --syntax scss \"scss/**/*.scss\" --cache --cache-location \"build/.stylelintcache/\"",
    "css:prefix": "postcss --config build/postcss.config.js --replace \"public/css/*.css\" \"public/css/!*.min.css\"",
    "css:compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/main.scss public/css/style.css",
    "css:minify": "cleancss --level 1 --source-map --source-map-inline-sources --output public/css/style.min.css public/css/style.css",
    "css": "npm run css:lint && npm run css:compile && npm run css:prefix && npm run css:minify",
    "css:watch": "watch 'npm run css' scss"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.1",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.2",
    "djv": "^2.1.0",
    "express": "^4.16.3",
    "express-favicon": "^2.0.1",
    "express-validator": "^5.2.0",
    "fs": "0.0.1-security",
    "helmet": "^3.12.1",
    "lodash": "^4.17.10",
    "marko": "^4.11.5",
    "method-override": "^2.3.10",
    "mime": "^2.3.1",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "morgan": "^1.9.0",
    "nconf": "^0.6.9",
    "nodemailer": "^4.6.7",
    "nodemailer-sendgrid": "^1.0.3",
    "pageres": "^4.5.1",
    "regex-weburl": "^0.1.0",
    "request": "^2.87.0",
    "request-ip": "^2.1.1",
    "serve-static": "^1.13.2",
    "simple-recaptcha-new": "^1.1.1"
  },
  "noAnalyze": true,
  "engines": {
    "node": ">=6.x.x <10",
    "npm": ">=5.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.4",
    "clean-css-cli": "^4.1.11",
    "node-sass": "^4.9.0",
    "postcss-cli": "^5.0.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-preset-env": "^5.2.1",
    "postcss-syntax": "^0.31.0",
    "semver": "^5.5.0",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.1.3",
    "watch": "^1.0.2"
  },
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ]
}
